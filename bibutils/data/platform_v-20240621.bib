	keywords = {Psychiatric disorders, Neuroscience, Learning and memory},
	pages = {1--14},
	file = {Full Text PDF:/home/ed203246/snap/zotero-snap/common/Zotero/storage/ZH5ZVT32/Ernst et al. - 2019 - Pubertal maturation and sex effects on the default.pdf:application/pdf},
