	month = apr,
	year = {2023},
	keywords = {IMAGEN, Adolescent health, COINSTAC, CVEDA, Decentralized analysis, Neuromark},
	pages = {287--301},
	file = {Version soumise:/home/ed203246/snap/zotero-snap/common/Zotero/storage/G2F8JJJP/Gazula et al. - 2023 - Federated Analysis in COINSTAC Reveals Functional .pdf:application/pdf},
