	keywords = {Humans, Adult, Female, Male, Cerebral Cortex, Depressive Disorder, Middle Aged, Functional Laterality, Psychiatric Status Rating Scales, Antidepressive Agents, Antimanic Agents, Drug Resistance, Electroconvulsive Therapy, Lithium Carbonate},
	pages = {127--135},
