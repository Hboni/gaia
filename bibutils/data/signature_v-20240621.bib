	pmcid = {PMC4148432},
	keywords = {Humans, Magnetic Resonance Imaging, Adult, Female, Male, Cerebral Cortex, Middle Aged, CADASIL, Organ Size, Case-Control Studies},
	pages = {e106311},
	file = {Full Text:/home/ed203246/snap/zotero-snap/common/Zotero/storage/JXEABKX4/De Guio et al. - 2014 - In vivo high-resolution 7 Tesla MRI shows early an.pdf:application/pdf},
}

