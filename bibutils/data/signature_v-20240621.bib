	pmid = {22000857},
	keywords = {Humans, Magnetic Resonance Imaging, Models, Neurological, Adult, Female, Male, Cerebral Cortex, Middle Aged, Nerve Fibers, Myelinated, Aged, Aged, 80 and over, Atrophy, CADASIL, Dementia, Vascular, Disease Progression, Follow-Up Studies, Longitudinal Studies},
