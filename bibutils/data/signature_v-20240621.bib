	pmid = {30242828},
	keywords = {structural MRI, schizophrenia, classification, first-episode psychosis, psychoradiology},
	pages = {571--580},
	file = {Full Text:/home/ed203246/snap/zotero-snap/common/Zotero/storage/V6MET7X7/de Pierrefeu et al. - 2018 - Identifying a neuroanatomical signature of schizop.pdf:application/pdf;Snapshot:/home/ed203246/snap/zotero-snap/common/Zotero/storage/7QP5YXYL/acps.html:text/html},
