	pmid = {24793184},
	pmcid = {PMC4105258},
	keywords = {Brain, Humans, Brain Mapping, Female, Male, Middle Aged, Aged, Cognition Disorders, Leukoencephalopathies, Cerebral Small Vessel Diseases, Trail Making Test},
	pages = {1946--1950},
