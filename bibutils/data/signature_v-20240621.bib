	number = {1},
	journal = {Translational Psychiatry},
	author = {Magalhães, R. and Bourgin, J. and Boumezbeur, F. and Marques, P. and Bottlaender, M. and Poupon, C. and Djemaï, B. and Duchesnay, E. and Mériaux, S. and Sousa, N. and Jay, T. M. and Cachia, A.},
